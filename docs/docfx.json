{
    "build": {
        "content": [
            {
                "files": [
                    "toc.yml",
                    "*.md"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "media/**"
                ]
            }
        ],
        "output": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "modern"
        ],
        "postProcessors": [],
        "keepFileLink": false,
        "globalMetadata": {
            "_appLogoPath": "media/logo.svg",
            "_appFaviconPath": "media/favicon.ico",
            "pdfCoverPage": "cover.html",
            "pdfTocPage": true,
            "pdf": true
        },
        "disableGitFeatures": false
    }
}